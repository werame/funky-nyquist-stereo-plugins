{
    "$schema": "./node_modules/@cspell/cspell-types/cspell.schema.json",
    "version": "0.2",
    "language": "en-US",
    "flagWords": [
        "hte"
    ],
    "ignorePaths": [
        ".git",
        ".gitignore",
        ".vscodeignore",
        "*.lock",
        "*.log",
        "languageCodes.ts",
        "package-lock.json"
    ],
    // obviously need a better solution than this
    "ignoreWords": [
        "setq",
        "defun",
        "arrayp",
        "mult"
    ],
    "languageSettings": [
        {
            "languageId": "lisp",
            "allowCompoundWords": true // for maketable and the like
        }
    ],
    "useGitignore": true,
    // this one gets auto-generated when you click "add words to cspell configuration"
    "words": [
        "cond"
    ]
}
